@model Department
@{
    ViewData["Title"] = "Details";
}

<h1>Details Of @Model.Name</h1>

    <div class="form-group">
        <label asp-for="@Model.Code" class="col-form-label" ></label>
        <input disabled asp-for="@Model.Code" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Name" class="col-form-label" ></label>
        <input disabled asp-for="@Model.Name" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="@Model.DateOfCreation" class="col-form-label" ></label>
        <input disabled asp-for="@Model.DateOfCreation" class="form-control"/>
    </div>
     
    <a></a>
    <a asp-action="Edit" class="btn btn-light">Edit </a>
    <a asp-action="Index" class="btn btn-secondary"> Back To List </a>